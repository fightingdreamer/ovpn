remote %{server.public.ip} %{server.public.port}
proto udp4
dev tun

persist-key
persist-tun

cipher CHACHA20-POLY1305
data-ciphers CHACHA20-POLY1305

pull
tls-client

resolv-retry infinite
nobind
remote-cert-tls server

verb 3

<ca>
%{ca.crt.content}
</ca>

<cert>
%{client.issued.crt.content}
</cert>

<key>
%{client.private.key.content}
</key>
